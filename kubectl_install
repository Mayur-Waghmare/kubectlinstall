1. curl -O https://s3.us-west-2.amazonaws.com/amazon-eks/1.27.1/2023-04-19/bin/linux/amd64/kubectl
2. chmod +x ./kubectl
3. mkdir -p $HOME/bin && cp ./kubectl $HOME/bin/kubectl && export PATH=$HOME/bin:$PATH
4. kubectl version --short --client
5. curl --silent --location "https://github.com/weaveworks/eksctl/releases/latest/download/eksctl_$(uname -s)_amd64.tar.gz" | tar xz -C /tmp
6. sudo mv /tmp/eksctl /usr/local/bin
7. eksctl version
8. eksctl create cluster --name nyn --node-type t2.micro --nodes 2 --region=us-east-1
